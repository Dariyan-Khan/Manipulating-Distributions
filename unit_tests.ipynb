   "source": [
    "f = x -> sin(3x) + sin(40x)\n",
    "n=31\n",
    "z = range(0, 2π, length=2n+1)[1:end-1]\n",
    "samples = f.(z)\n",
    "A = fftmatrix(n)\n",
    "f̂ = (1 / sqrt(2n)) * A * samples\n",
    "\n",
    "plot(0:length(f̂)-1, abs.(f̂))"
   ]
